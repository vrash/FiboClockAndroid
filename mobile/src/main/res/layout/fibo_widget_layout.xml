<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/wfivesquare"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/border"

        android:layout_alignTop="@+id/wtwosquare"
        android:layout_alignParentEnd="true" />
    <Button
        android:id="@+id/wonesqaure"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/border"

        android:layout_alignTop="@+id/wfivesquare"
        android:layout_toStartOf="@+id/wfivesquare" />
    <Button
        android:id="@+id/wotheronesquare"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:background="@drawable/border"

        android:layout_toStartOf="@id/wfivesquare"
        android:layout_marginTop="129dp" />
    <Button
        android:id="@+id/wtwosquare"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/border"

        android:layout_alignBottom="@+id/wotheronesquare"
        android:layout_alignStart="@+id/wthreesquare" />
    <Button
        android:id="@+id/wthreesquare"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/border"
        android:layout_alignBottom="@id/wfivesquare"
        android:layout_alignEnd="@id/wonesqaure" />
</RelativeLayout>